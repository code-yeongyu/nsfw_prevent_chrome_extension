{
  "name": "NSFW_PREVENTION",
  "version": "0.1",
  "manifest_version" : 2,
  "description": "Censor some bad images!",
  	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"replace.js"
			],
			"run_at": "document_idle"
		}
	],
  "permissions": [
    "background",
    "tabs",
    "activeTab",
    "http://*/",
    "https://*/",
    "*://*/*",
    "<all_urls>"
  ]
}
